# База данных PostgreSQL
POSTGRES_USER=asu_user
POSTGRES_PASSWORD=asu_password
POSTGRES_DB=asu_db

# Для локальной разработки (без Docker)
DATABASE_URL=postgresql://asu_user:asu_password@localhost:5432/asu_db

# Секрет для сессий (обязательно измените в production!)
SESSION_SECRET=your-super-secret-session-key-change-in-production

# SMS-PROSTO API (обязательно для отправки SMS)
SMS_API_KEY=your-sms-prosto-api-key
SMS_SENDER=INFO

# Окружение
NODE_ENV=production
PORT=5000
